[user]
	name = bob-icsg
	email = bobzhou@bu.edu
[core]
	excludesfile = /home/bobzhou/.gitignore_global
	editor = vim
[alias]
    a = add
    c = checkout
    cm = commit -m
    b = branch
    pl = pull
    ps = push 
    lg = log --graph --abbrev-commit --decorate --date=relative --format=format:'%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(dim white)- %an%C(reset)%C(bold yellow)%d%C(reset)' --all
    bare = config --bool core.bare true
    send = !git add . &&  git commit -m \"AUTOCOMMIT `whoami`@`hostname`:`date`\" && git push origin master
[web]
    browser = google-chrome
[instaweb]
    httpd = apache2 -f

